

.item {
  order: 7; /* default is 0 */
}

.body-main{
	margin: 0;
}

.div-header-menu{
	max-width:1440px;
    min-width: 1024px;
	height: 120px;
}
.nav-menu{
	display: flex;
    margin-right: 80px;
    margin-left: 80px;
    -webkit-box-pack: center;
    justify-content: center;
}

.c-nav-link{
	transition: color .2s;
    #font-family: Montserrat,sans-serif;
    font-size: 18px;
    font-weight: 400;
	width: 112px;
	height: 60px;
	#top: 30px;
    #text-align: center;
	
	#position: absolute;
    display: inline-block;
    vertical-align: top;
    text-decoration: none;
    color: #112E66
    padding: 30px;
    margin-left: auto;
    margin-right: auto
}

.c-nav-link:hover {
	text-decoration: none;
	border-bottom: 5px solid #FFB800;
}

.center-content{
	margin: 5px 0 0;
    min-height: 700px;
    justify-content: center;
    display: flex;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: cover;
	background:#001a4d;
	opacity: 0.9;
}
.div-center-title{
	padding: 200px;
	width: 500px;
	height: 130px;
	font-style: normal;
	font-size:56px;
	line-height: 66px;
    flex-direction: column;
    justify-content: center;
	text-align: center;
	color: #FFFFFF;
	letter-spacing: -1px;
}

.div-center-content{
	padding: 360px;
	color: #FFFFFF;
	width: 580px;
}

.div-middle-menu{
	max-width:1440px;
    min-width: 1024px;
	height: 112px;
}